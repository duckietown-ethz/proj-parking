<launch>
  <include file="$(find parking)/launch/lane_following.launch"/>

  <group ns="$(arg veh)">
    <node pkg="parking" type="parking.py" name="parking" output="screen"/>
    <node pkg="parking" type="free_parking_detection.py" name="parking_free" output="screen"/>
    <node pkg="parking" type="stop_parking_detection.py" name="parking_stop" output="screen"/>
  </group>
</launch>
